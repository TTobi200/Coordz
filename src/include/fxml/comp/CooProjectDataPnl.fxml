<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<VBox BorderPane.alignment="CENTER">
			<children>
				<TitledPane text="Projekt" >
					<GridPane vgap="5" hgap="5">
							<columnConstraints>
								<ColumnConstraints hgrow="NEVER" />
								<ColumnConstraints hgrow="ALWAYS" />
							</columnConstraints>
							<rowConstraints>
							</rowConstraints>
							<children>
								<Label text="Name" />
								<Label text="Datum" GridPane.rowIndex="1" />
								<TextField fx:id="txtPrjName" GridPane.columnIndex="1" />
								<TextField fx:id="txtPrjDate" GridPane.columnIndex="1"
									GridPane.rowIndex="1" />
							</children>
						</GridPane>
				</TitledPane>
				<TitledPane text="LAP Software" >
					<GridPane vgap="5" hgap="5">
							<columnConstraints>
								<ColumnConstraints hgrow="NEVER" />
								<ColumnConstraints hgrow="ALWAYS" />
							</columnConstraints>
							<rowConstraints>
							</rowConstraints>
							<children>
								<Label text="Name" />
								<Label text="Version" GridPane.rowIndex="1" />
								<TextField fx:id="txtSoftName" GridPane.columnIndex="1" />
								<TextField fx:id="txtSoftVersion" GridPane.columnIndex="1"
									GridPane.rowIndex="1" />
							</children>
						</GridPane>
				</TitledPane>
			</children>
		</VBox>
	</center>
</fx:root>
